#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/srt-parser-2@1.2.3/node_modules/srt-parser-2/bin/node_modules:/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/srt-parser-2@1.2.3/node_modules/srt-parser-2/node_modules:/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/srt-parser-2@1.2.3/node_modules:/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/srt-parser-2@1.2.3/node_modules/srt-parser-2/bin/node_modules:/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/srt-parser-2@1.2.3/node_modules/srt-parser-2/node_modules:/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/srt-parser-2@1.2.3/node_modules:/data/data/com.termux/files/home/cloudinary-video-player-master/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
